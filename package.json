{
  "name": "sqltools-odbc",
  "displayName": "SqlTools Odbc",
  "description": "Odbc Driver Support for SqlTools",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.42.0"
  },
  "publisher": "tobi187",
  "license": "MIT",
  "repository": "https://github.com/tobi187/SqlTools-Odbc",
  "bugs": {
    "url": "https://github.com/tobi187/SqlTools-Odbc/issues"
  },
  "icon": "icons/db-orange.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "keywords": [
    "driver-template",
    "sqltools-driver"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "extensionDependencies": [
    "mtxr.sqltools"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "dependencies": {
    "@sqltools/base-driver": "latest",
    "@sqltools/types": "latest",
    "odbc": "^2.4.9",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/node": "^14.0.9",
    "@types/vscode": "^1.42.0",
    "typescript": "^3.7.3"
  }
}